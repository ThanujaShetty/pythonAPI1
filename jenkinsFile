pipeline {
    agent any

    stages {
        stage('git') {
            steps {
                git 'https://github.com/ThanujaShetty/pythonAPI1.git'
            }
        }
         stage('Tests') {
            steps {
               bat '''pip install -r requirement.txt
               cd SimpleBooks
               python test_simple_books.py'''
            }
        }
    }
}
